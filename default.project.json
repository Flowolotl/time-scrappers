{
  "name": "time-scrappers",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Packages": {
        "$path": "Packages"
      },
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "Game": {
        "$className": "Folder",
        "Modules": {
          "$className": "Folder"
        },
        "ServerRuntime": {
          "$path": "src/server/game/server_runtime"
        }
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": { 
        "Game": {
          "$className": "Folder",
          "Components": {
            "$className": "Folder",
            "Corner": {
              "$path": "src/client/game/components/Corner.luau"
            }
          },
          "Modules": {
            "$className": "Folder"
          },
          "ClientRuntime": {
            "$path": "src/client/game/client_runtime"
          }
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}